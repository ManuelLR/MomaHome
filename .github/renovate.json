{
    "baseBranches": ["next"],
    "platform": "github",
    "includeForks": false,
    "repositories": [
        "ManuelLR/MomaHome"
    ],
    "assignees": ["ManuelLR"],
    "reviewers": ["ManuelLR"],
    "regexManagers": [
        {
          "fileMatch": ["^Dockerfile$"],
          "matchStrings": [
            "datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s *?CUSTOM_.*?_VERSION=(?<currentValue>.*)\\s"
          ],
          "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
        }
    ]
}
